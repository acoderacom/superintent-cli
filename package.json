{
  "name": "superintent",
  "version": "0.0.2",
  "description": "CLI companion for the Superintent Claude Code plugin",
  "author": "Acodera",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/acodera/superintent-cli.git"
  },
  "homepage": "https://github.com/acodera/superintent-cli#readme",
  "bugs": {
    "url": "https://github.com/acodera/superintent-cli/issues"
  },
  "type": "module",
  "bin": {
    "superintent": "bin/superintent.js"
  },
  "scripts": {
    "build": "tsc && npm run build:css",
    "build:css": "npx @tailwindcss/cli -i src/ui/styles/input.css -o dist/ui/styles.css --minify",
    "dev": "tsx src/index.ts",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@hono/node-server": "^1.19.9",
    "@huggingface/transformers": "^3.8.1",
    "@libsql/client": "^0.17.0",
    "commander": "^14.0.3",
    "hono": "^4.11.9",
    "open": "^11.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@tailwindcss/cli": "^4.1.18",
    "@types/node": "^25.2.3",
    "eslint": "^9.0.0",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "dist/ui/styles.css",
    "bin"
  ],
  "keywords": [
    "superintent",
    "cli",
    "tickets",
    "knowledge",
    "embeddings",
    "claude"
  ],
  "license": "MIT"
}
